params {
    appsConfig {
        antifam {
            name = "AntiFam"
            invalid_chars = "-"
            runner = "hmmer"
            hmm = "antifam/AntiFam.hmm"
        }
        cdd {
            name = "CDD"
            runner = "rpsblast"
            rpsblast_db = "cdd/db/Cdd_NCBI"
            rpsproc_db = "cdd/data"
        }
        coils {
            name = "COILS"
            runner = "ncoils"
        }
        cathgene3d {
            name = "CATH-Gene3D"
            aliases = ["gene3d"]
            invalid_chars = "-."
            runner = "hmmer"
            hmm = "cath/gene3d/gene3d_main.hmm"
            model2sfs = "cath/gene3d/model_to_family_map.tsv"
            disc_regs = "cath/gene3d/discontinuous_regs.pkl.py3"
        }
        cathfunfam {
            name = "CATH-FunFam"
            aliases = ["funfam"]
            invalid_chars = "-."
            runner = "hmmer"
            dir = "cath/funfam/"
            chunkSize = 1000
        }
        hamap {
            name = "HAMAP"
            invalid_chars = "-."
            runner = "hmmer"
            hmm = "hamap/hamap.hmm.lib"
            dir = "hamap/profiles"
        }
        mobidblite {
            name = "MobiDB-lite"
            runner = "idrpred"
        }
        ncbifam {
            name = "NCBIFAM"
            invalid_chars = "-"
            runner = "hmmer"
            hmm = "ncbifam/ncbifam.hmm"
        }
        panther {
            name = "PANTHER"
            invalid_chars = "-"
            runner = "hmmer"
            hmm = "panther/famhmm/binHmm"
            msf = "panther/Tree_MSF"
            paint = "panther/PAINT_Annotations"
        }
        phobius {
            name = "Phobius"
            invalid_chars = "-*.OXUZJ"
            dir = ""
        }
        pfam {
            name = "Pfam"
            invalid_chars = "-"
            hmm = "pfam/pfam_a.hmm"
            seed = "pfam/pfam_a.seed"
            clan = "pfam/pfam_clans"
            dat = "pfam/pfam_a.dat"
        }
        pirsf {
            name = "PIRSF"
            invalid_chars = "-"
            hmm = "pirsf/sf_hmm_all"
            postprocess {
                data = "pirsf/pirsf.dat"
            }
        }
        pirsr {
            name = "PIRSR"
            invalid_chars = "-."
            hmm = "pirsr/sr_hmm_all"
            rules = "pirsr/sr_uru.json"
        }
        prints {
            name = "PRINTS"
            pval = "prints/prints.pval"
            hierarchy = "prints/FingerPRINTShierarchy.db"
        }
        prositepatterns {
            name = "PROSITE patterns"
            runner = "prosite_patterns"
            data = "prosite/prosite_patterns.dat"
            evaluator = "prosite/evaluator.dat"
        }
        prositeprofiles {
            name = "PROSITE profiles"
            invalid_chars = "-."
            runner = "prosite_profiles"
            data = "prosite/prosite_profiles"
            skip_flagged_profiles = "prosite/skip_flagged_profiles.txt"
        }
        sfld {
            name = "SFLD"
            invalid_chars = "-."
            hmm = "sfld/sfld.hmm"
            hierarchy = "sfld/sfld_hierarchy_flat.txt"
            sites_annotation = "sfld/sfld_sites.annot"
        }
        smart {
            name = "SMART"
            runner = "hmmer"
            hmm = "smart/smart.HMMs"
            hmmbin = "smart/smart.HMMs.bin"
        }
        superfamily {
            name = "SUPERFAMILY"
            invalid_chars = "-"
            hmm = "superfamily/hmmlib_1.75"
            selfhits = "superfamily/self_hits.tab"
            cla = "superfamily/dir.cla.scop.txt_1.75"
            model = "superfamily/model.tab"
            pdbj95d = "superfamily/pdbj95d"
        }
        signalp_euk {
            name = "SignalP-Euk"
            organism = "eukarya"
            dir = ""
            mode = "fast"
        }
        signalp_prok {
            name = "SignalP-Prok"
            organism = "other"
            dir = ""
            mode = "fast"
        }
        tmhmm {
            name = "TMHMM"
            dir = ""
        }
    }
    xRefsConfig {
        entries = "xrefs/entries.json"
        goterms = "xrefs/goterms"
        pathways = "xrefs/pathways"
    }
}

