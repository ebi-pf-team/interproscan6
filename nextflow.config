params {
    ipsc_version = '6.0.0'
    nucleic = false
    batchsize = 100
    help = false
    applications = 'AntiFam,CDD,FunFam,Gene3D,HAMAP,NCBIfam,Panther,Pfam,PROSITE_Profiles,SFLD,SignalP,TMHMM'
    disable_precalc = false
    formats = 'json,tsv,xml'
    output = 'results/output'
    goterms = false
    pathways = false
    translate {
        strand = 'both'
        methionine = false
        min_len = 30
        genetic_code = 1
    }
    xrefs {
        entries="$projectDir/data_xref/entries"
        goterms="$projectDir/data_xref/goterms"
        pathways="$projectDir/data_xref/pathways"
    }
}

process.container = 'interproscan6'
docker {
    enabled = true
    mountFlags = 'Z'
}

includeConfig 'subworkflows/sequence_precalc/lookup.config'
includeConfig 'subworkflows/sequence_analysis/members.config'
